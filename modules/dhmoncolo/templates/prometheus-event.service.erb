[Unit]
Description=Prometheus Colo
After=syslog.target network.target

[Service]
EnvironmentFile=-/etc/defaults/prometheus-event
User=prometheus
WorkingDirectory=/opt/prometheus-event
ExecStart=/opt/prometheus-event/prometheus $ARGS

[Install]
WantedBy=multi-user.target

