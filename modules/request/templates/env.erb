APP_NAME=TechRequests
APP_ENV=local
APP_KEY=<%= @dhid['app_key'] %>
APP_DEBUG=false
APP_LOG_LEVEL=info
APP_URL=https://<%= @fqdn %>

DB_CONNECTION=pgsql
DB_HOST=::1
DB_PORT=5432
DB_DATABASE=<%= @dbname %>
DB_USERNAME=<%= @db['username'] %>
DB_PASSWORD=<%= @db['password'] %>

BROADCAST_DRIVER=log
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_DRIVER=sync

REDIS_HOST=::1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp
MAIL_HOST=::1
MAIL_PORT=25
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@<%= @fqdn %>"
MAIL_FROM_NAME="Tech request system"

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=

API_CLIENT_ID=<%= @dhid['dhid_client'] %>
API_SECRET=<%= @dhid['dhid_secret'] %>
DHID_CLIENT=<%= @dhid['dhid_client'] %>
DHID_SECRET=<%= @dhid['dhid_secret'] %>
DHID_REDIRECT=https://<%= @fqdn %>/login/dhid/callback

DEBUGBAR_ENABLED=false
